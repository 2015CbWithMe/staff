<view class="wrap income_wrap"><view class="status_bar index_status_bar"></view><customnav vue-id="1" ismsg="{{true}}" isSearch="{{false}}" midtitle="{{true}}" navtitle="收入管理" cancletext="{{false}}" focus="{{false}}" bind:__l="__l"></customnav><view class="income_header"><view data-event-opts="{{[['tap',[['showTimepicker',['$event']]]]]}}" class="timePick" bindtap="__e">{{monthVal+''}}<view class="iconfont icon-icon-arrow-bottom2 _i"></view></view><view class="shopReturn"><label class="_span">推荐小店采购返点</label><view class="_h2"><label class="_span">￥</label>{{recommendRefund}}</view><label class="_span">{{"推荐小店采购总额:"+recommendTotal}}</label></view></view><scroll-view class="income_list" scroll-y="true"><block wx:for="{{incomeList}}" wx:for-item="item" wx:for-index="idx" wx:key="idx"><view data-event-opts="{{[['tap',[['gotoIncomeDetail',['$0','$1'],[[['incomeList','',idx,'times']],[['incomeList','',idx,'fee']]]]]]]}}" class="income_list_item" bindtap="__e"><view class="_ul"><view class="_li">{{item.times}}</view><view class="_li">平台返点</view><view class="_li">{{item.fee+''}}<view class="iconfont icon-you _i"></view></view></view></view></block><view class="emptyTips"><block wx:if="{{incomeList.length==0}}"><label class="_span">{{$root.m0+"月份没有收入哦 :("}}</label></block></view></scroll-view><qspicker class="vue-ref" vue-id="2" type="date" dataSet="{{dateSet}}" data-ref="picker" data-event-opts="{{[['^confirm',[['picked',['$event']]]]]}}" bind:confirm="__e" bind:__l="__l"></qspicker></view>