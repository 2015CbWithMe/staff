<view class="wrap current_account_wrap"><view class="status_bar index_status_bar"></view><customnav vue-id="1" ismsg="{{true}}" isSearch="{{false}}" midtitle="{{true}}" navtitle="修改登录密码" cancletext="{{false}}" focus="{{false}}" bind:__l="__l"></customnav><view class="current_account_list"><view class="_p">为保障您的数据安全，修改密码前请填写原密码</view><view class="current_account_list_item"><label class="_span">原密码</label><block wx:if="{{inp1==='checkbox'}}"><input class="codeInp" focus="true" placeholder="请输入原登录密码" type="checkbox" data-event-opts="{{[['change',[['e0',['$event']]]]]}}" checked="{{$root.g0?_i(oldPsw,null)>-1:oldPsw}}" bindchange="__e"/></block><block wx:else><block wx:if="{{inp1==='radio'}}"><input class="codeInp" focus="true" placeholder="请输入原登录密码" type="radio" data-event-opts="{{[['change',[['e1',['$event']]]]]}}" checked="{{_q(oldPsw,null)}}" bindchange="__e"/></block><block wx:else><input class="codeInp" focus="true" placeholder="请输入原登录密码" type="{{inp1}}" data-event-opts="{{[['input',[['__set_model',['','oldPsw','$event',[]]]]]]}}" value="{{oldPsw}}" bindinput="__e"/></block></block><view data-event-opts="{{[['tap',[['eyechange',[1]]]]]}}" class="{{['iconfont _i',eyeopen1?'icon-eye1':'icon-eye']}}" bindtap="__e"></view></view><view class="current_account_list_item"><label class="_span">新密码</label><block wx:if="{{inp2==='checkbox'}}"><input class="codeInp" placeholder="请输入新设置登录密码" type="checkbox" data-event-opts="{{[['change',[['e2',['$event']]]]]}}" checked="{{$root.g1?_i(newPsw,null)>-1:newPsw}}" bindchange="__e"/></block><block wx:else><block wx:if="{{inp2==='radio'}}"><input class="codeInp" placeholder="请输入新设置登录密码" type="radio" data-event-opts="{{[['change',[['e3',['$event']]]]]}}" checked="{{_q(newPsw,null)}}" bindchange="__e"/></block><block wx:else><input class="codeInp" placeholder="请输入新设置登录密码" type="{{inp2}}" data-event-opts="{{[['input',[['__set_model',['','newPsw','$event',[]]]]]]}}" value="{{newPsw}}" bindinput="__e"/></block></block><view data-event-opts="{{[['tap',[['eyechange',[2]]]]]}}" class="{{['iconfont _i',eyeopen2?'icon-eye1':'icon-eye']}}" bindtap="__e"></view></view><view class="current_account_list_item"><label class="_span">确认密码</label><block wx:if="{{inp3==='checkbox'}}"><input class="codeInp" placeholder="请再次输入新设置登录密码" type="checkbox" data-event-opts="{{[['change',[['e4',['$event']]]]]}}" checked="{{$root.g2?_i(confirmPsw,null)>-1:confirmPsw}}" bindchange="__e"/></block><block wx:else><block wx:if="{{inp3==='radio'}}"><input class="codeInp" placeholder="请再次输入新设置登录密码" type="radio" data-event-opts="{{[['change',[['e5',['$event']]]]]}}" checked="{{_q(confirmPsw,null)}}" bindchange="__e"/></block><block wx:else><input class="codeInp" placeholder="请再次输入新设置登录密码" type="{{inp3}}" data-event-opts="{{[['input',[['__set_model',['','confirmPsw','$event',[]]]]]]}}" value="{{confirmPsw}}" bindinput="__e"/></block></block><view data-event-opts="{{[['tap',[['eyechange',[3]]]]]}}" class="{{['iconfont _i',eyeopen3?'icon-eye1':'icon-eye']}}" bindtap="__e"></view></view></view><view class="nextMove"><button type="warn" disabled="{{disable}}" data-event-opts="{{[['tap',[['changePsw',['$event']]]]]}}" bindtap="__e">确定</button></view></view>