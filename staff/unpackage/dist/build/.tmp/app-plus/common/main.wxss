.wrap {
  width: 100vw;
  height: 100vh;
  background: #f6f6f6;
}
button {
  margin: 0;
}
._a {
  display: inline-block;
  color: #f8f8f8;
  text-decoration: none;
}
.status_bar {
  width: 100%;
  height: var(--status-bar-height);
  background: -webkit-gradient(linear, left top, right top, from(#21A5F9), to(#1A6FE8));
